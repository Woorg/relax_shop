		url('../fonts/os/hinted-subset-OpenSans-Semibold.woff') format('woff');
	font-weight: normal;
	font-style: normal;
}

$osL = 'OpenSans-Light', Arial, Helvetica Neue, Helvetica, sans-serif
$osR = 'OpenSans-Reg', Arial, Helvetica Neue, Helvetica, sans-serif
$osSb = 'OpenSans-Semibold', Arial, Helvetica Neue, Helvetica, sans-serif
