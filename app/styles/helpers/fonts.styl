@font-face {
	font-family: 'OpenSans-Bold';
	src: url('../fonts/os/hinted-subset-OpenSans-Bold.woff2') format('woff2'),
		url('../fonts/os/hinted-subset-OpenSans-Bold.woff') format('woff');
	font-weight: normal;
	font-style: normal;
}

@font-face {
	font-family: 'OpenSans-Light';
	src: url('../fonts/os/hinted-subset-OpenSans-Light.woff2') format('woff2'),
		url('../fonts/os/hinted-subset-OpenSans-Light.woff') format('woff');
	font-weight: normal;
	font-style: normal;
}

@font-face {
	font-family: 'Lobster-Regular';
	src: url('../fonts/lob/hinted-subset-Lobster-Regular.woff2') format('woff2'),
		url('../fonts/lob/hinted-subset-Lobster-Regular.woff') format('woff');
	font-weight: normal;
	font-style: normal;
}

@font-face {
	font-family: 'OpenSans-Reg';
	src: url('../fonts/os/hinted-subset-OpenSans.woff2') format('woff2'),
		url('../fonts/os/hinted-subset-OpenSans.woff') format('woff');
	font-weight: normal;
	font-style: normal;
}

@font-face {
	font-family: 'OpenSans-Semibold';
